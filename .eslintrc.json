{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "no-plusplus": [2, {"allowForLoopAfterthoughts": true }],
        "indent": ["error", 4],
        "import/extensions":"off",
        "import/no-extraneous-dependencies":"off"
    }
}
